{
  "apiVersion": 2,
  "name": "aab/horizontal-accordion",
  "version": "0.0.1",
  "title": "Horizontal Accordion",
  "category": "accordion-block",
  "description": "Build unique and modern horizontal style accordions",
  "supports": {
    "spacing": {
      "padding": true,
      "margin": true
    },
    "color": {
      "color": true,
      "background": true
    }
  },
  "attributes": {
    "activatorEvent": {
      "type": "string",
      "default": "hover"
    },
    "activeItemsClientId": {
      "type": "string"
    }
  },
  "providesContext": {
    "aahb/accordion-activeItemsClientId": "activeItemsClientId"
  },
  "textdomain": "advanced-accordion-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": [
    "file:./style-index.css"
  ],
  "viewScript": [
    "aahb-horizontal-accordion"
  ]
}